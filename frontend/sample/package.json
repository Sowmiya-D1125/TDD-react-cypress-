    "cypress-axe": "^1.4.0",
    "cypress-localstorage-commands": "^2.2.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
